{
    "name": "traderinteractive/psr-log-slack",
    "description": "A PSR-3 logger implementation logging to slack",
    "keywords": ["psr3", "log", "slack"],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "chadicus/psr-log-helper": "^1.2",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "chadicus/coding-standard": "^1.3",
        "php-coveralls/php-coveralls": "^1.0",
        "phpunit/phpunit": "^6.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "TraderInteractive\\Psr\\Log\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TraderInteractiveTest\\Psr\\Log\\": "tests"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs",
        "test": "vendor/bin/phpunit"
    }
}
